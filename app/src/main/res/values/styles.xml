    <style name="MatButtonDelete" parent="Theme.MaterialComponents">
        <item name="colorOnPrimary">@android:color/holo_red_light</item>
        <item name="colorPrimary">#eb2f06</item>
        <item name="colorOnSurface">#ee5253</item>
    </style>


</resources>
